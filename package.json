{
  "name": "angular2-playground",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && webpack --inline --colors --progress --display-error-details --display-cached",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 8000",
    "start": "npm run server",
    "watch": "npm run build -- --watch"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.21"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.0",
    "@types/node": "^6.0.38",
    "angular2-template-loader": "^0.5.0",
    "autoprefixer": "^6.4.1",
    "awesome-typescript-loader": "^2.2.4",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "postcss-loader": "^0.13.0",
    "raw-loader": "0.5.1",
    "rimraf": "^2.5.4",
    "ts-helpers": "^1.1.1",
    "typescript": "^2.0.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  }
}
